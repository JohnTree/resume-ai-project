{
  "version": 2,
  "builds": [
    {
      "src": "demo.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/index.html", 
      "use": "@vercel/static"
    },
    {
      "src": "public/resume-list.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/debug.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/editor",
      "dest": "/public/index.html"
    },
    {
      "src": "/resumes",
      "dest": "/public/resume-list.html"
    },
    {
      "src": "/debug",
      "dest": "/public/debug.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "https://resume-ai-project-production.up.railway.app/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}