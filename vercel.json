{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://resume-ai-project-production.up.railway.app/api/$1"
    },
    {
      "src": "/resumes",
      "dest": "/resume-list.html"
    },
    {
      "src": "/debug",
      "dest": "/debug.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}